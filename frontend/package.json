{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:switch": "tsx scripts/switch-db.ts",
    "db:use:dev": "tsx scripts/switch-db.ts dev",
    "db:use:prod": "tsx scripts/switch-db.ts prod",
    "db:status": "tsx scripts/db-status.ts",
    "db:seed:dev": "DB_ENV=dev tsx src/lib/db/seed.ts",
    "db:seed:prod": "DB_ENV=prod tsx src/lib/db/seed.ts",
    "db:migrate:dev": "DB_ENV=dev drizzle-kit migrate",
    "db:migrate:prod": "DB_ENV=prod drizzle-kit migrate",
    "db:push:dev": "DB_ENV=dev drizzle-kit push",
    "db:push:prod": "DB_ENV=prod drizzle-kit push",
    "build:prod": "next build",
    "debug:build": "NODE_OPTIONS='--inspect' tsx scripts/debug-build.ts",
    "deploy": "npm run db:use:prod && vercel --prod"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@prisma/client": "^6.8.2",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "minisearch": "^7.1.2",
    "next": "15.3.2",
    "pg": "^8.11.3",
    "postgres": "^3.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "server-only": "^0.0.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.15",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prisma": "^6.8.2",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5"
  }
}
